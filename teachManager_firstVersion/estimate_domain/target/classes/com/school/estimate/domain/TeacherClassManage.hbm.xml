<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.school.estimate.domain">
	<class name="TeacherClassManage" table="TEACHER_CLASS_MANAGE" dynamic-insert="true" dynamic-update="true">
		<id name="id" column="TEACHER_CLASS_MANAGE_ID">
			<generator class="uuid"/>
		</id>

		<property name="classId" column="CLASS_ID"/>
		<property name="subjectId" column="SUBJECT_ID"/>
		<property name="teacherId" column="TEACHER_ID"/>
		
	</class>
</hibernate-mapping>    