<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.school.estimate.domain">
	<class name="Estimate" table="ESTIMATE" dynamic-insert="true" dynamic-update="true">
		<id name="id" column="ESTIMATE_ID">
			<generator class="uuid"/>
		</id>

		<property name="name" column="NAME"/>
		<property name="dimension" column="DIMENSION"/>
		<property name="element" column="ELEMENT"/>
		<property name="maxValue" column="MAX_VALUE" />
		<property name="singleValue" column="SINGLE_VALUE" />
		
	</class>
</hibernate-mapping>    