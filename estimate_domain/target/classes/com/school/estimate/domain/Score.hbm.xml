<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.school.estimate.domain">
	<class name="Score" table="SCORE" dynamic-insert="true" dynamic-update="true">
		<id name="id" column="SCORE_ID">
			<generator class="uuid"/>
		</id>

		<property name="estimateId" column="ESTIMATE_ID"/>
		<property name="studentId" column="STUDENT_ID"/>
		<property name="term" column="TERM"/>
		<property name="grade" column="GRADE" />
		<property name="teacherId" column="TEACHER_ID" />
		<property name="time" column="TIME" />
		<property name="sorbName" column="SORB_NAME" />
		
	</class>
</hibernate-mapping>    